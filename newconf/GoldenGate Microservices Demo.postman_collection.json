		{
			"name": "Create EXCA extract",
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "welcome1",
							"type": "string"
						},
						{
							"key": "username",
							"value": "ggadmin",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"description\":\"Extract for HR schema in CADB created using RESTful call\",\n    \"config\":[\n        \"Extract    \tEXCA\",\n        \"ExtTrail   \tec\",\n        \"useridalias\tcdbogg domain cdbogg\",\n        \"sourcecatalog\tcadb\",\n        \"Table      \thr.*;\"\n    ],\n    \"source\":{\n        \"tranlogs\":\"integrated\"\n    },\n    \"credentials\":{\n    \t\"domain\":\"cdbogg\",\n        \"alias\":\"cdbogg\"\n    },\n    \"registration\": {\n            \"containers\": [\n                \"CADB\"\n            ]\n    },\n    \"begin\":\"now\",\n    \"targets\":[\n        {\n            \"name\":\"ec\",\n            \"sizeMB\":100\n        }\n    ],\n    \"status\":\"stopped\"\n}"
